{
    "name": "tarre/laravel-swish",
    "description": "Laravel wrapper for Swish",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "laravel/laravel": "^5.0|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Tarre\\Swish\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarre\\Swish\\Providers\\SwishServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tarre Islam",
            "email": "tarre.islam@gmail.com"
        }
    ]
}
